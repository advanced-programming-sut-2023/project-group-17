   <children>
      <Button layoutX="187.0" layoutY="119.0" mnemonicParsing="false" text="New" onMouseClicked="#openNewTrade">
         <font>
            <Font name="Showcard Gothic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="374.0" layoutY="119.0" mnemonicParsing="false" text="History" onMouseClicked="#openTradeHistory">
         <font>
            <Font name="Showcard Gothic" size="12.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" text="X" onMouseClicked="#closeTradeMenu">
         <font>
            <Font name="Showcard Gothic" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
