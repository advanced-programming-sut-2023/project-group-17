<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.FXCollections?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Client.view.AvatarMenu"
            prefHeight="1080.0" prefWidth="1920.0">
    <Button text="Back To Profile Menu" prefWidth="160" onAction="#backToProfile" fx:id="backToProfile" layoutY="0.0"
    layoutX="0.0" style="-fx-background-color: #daa520;" />
    <ChoiceBox fx:id="avatarOtherPlayers" layoutX="180.0" layoutY="300.0" prefHeight="26.0" prefWidth="220.0"
               value="Choose people to get their avatars" onMouseClicked="#getOtherPlayersAvatar" style="-fx-background-color: #daa520;">
        <FXCollections fx:factory="observableArrayList"/>
    </ChoiceBox>

</AnchorPane>
